{
	"name": "chat-api",
	"version": "2.1.74",
	"description": "chat-api",
	"author": "",
	"private": true,
	"license": "MIT",
	"scripts": {
		"prebuild": "rimraf dist",
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "ENV_PATH=.local.env nest start",
		"start:dev": "ENV_PATH=.local.env nest start --watch",
		"start:debug": "ENV_PATH=.local.env nest start --debug --watch",
		"start:prod": "nest start",
		"tsc": "tsc",
		"lint": "eslint",
		"test": "jest",
		"test:int": "jest --config ./src/jest.json",
		"test:watch": "jest --watch --config ./src/jest.json",
		"test:cov": "jest --coverage --config ./src/jest.json",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "ENV_PATH=.local.env NODE_ENV=test jest --config ./test/jest-e2e.json",
		"test:all": "jest"
	},
	"dependencies": {
		"@nestjs/common": "11.1.8",
		"@nestjs/config": "4.0.2",
		"@nestjs/core": "11.1.8",
		"@nestjs/jwt": "11.0.1",
		"@nestjs/passport": "11.0.5",
		"@nestjs/platform-express": "11.1.8",
		"@nestjs/platform-socket.io": "11.1.8",
		"@nestjs/swagger": "11.2.1",
		"@nestjs/typeorm": "11.0.0",
		"@nestjs/websockets": "11.1.8",
		"blurhash": "2.0.5",
		"passport": "0.7.0",
		"passport-jwt": "4.0.1",
		"passport-local": "1.0.0",
		"pg": "8.16.3",
		"postgres": "3.4.7",
		"reflect-metadata": "0.2.2",
		"rimraf": "6.0.1",
		"rxjs": "7.8.2",
		"sharp": "0.34.4",
		"swagger-ui-express": "5.0.1",
		"typeorm": "0.3.27",
		"typevert": "0.9.11"
	},
	"devDependencies": {
		"@eslint/eslintrc": "3.3.1",
		"@eslint/js": "9.38.0",
		"@faker-js/faker": "9.9.0",
		"@nestjs/cli": "11.0.10",
		"@nestjs/schematics": "11.0.9",
		"@nestjs/testing": "11.1.8",
		"@types/express": "5.0.4",
		"@types/jest": "30.0.0",
		"@types/multer": "2.0.0",
		"@types/node": "24.9.1",
		"@types/passport-jwt": "4.0.1",
		"@types/passport-local": "1.0.38",
		"@types/supertest": "6.0.3",
		"@typescript-eslint/eslint-plugin": "8.46.2",
		"@typescript-eslint/parser": "8.46.2",
		"eslint": "9.38.0",
		"eslint-config-prettier": "10.1.8",
		"eslint-plugin-prettier": "5.5.4",
		"globals": "16.4.0",
		"jest": "30.2.0",
		"prettier": "3.6.2",
		"sqlite3": "5.1.7",
		"supertest": "7.1.4",
		"ts-jest": "29.4.5",
		"ts-loader": "9.5.4",
		"ts-node": "10.9.2",
		"tsconfig-paths": "4.2.0",
		"typescript": "5.9.3",
		"typescript-eslint": "8.46.2"
	},
	"jest": {
		"projects": [
			"src/jest.json",
			"test/jest-e2e.json"
		]
	}
}
