		"postgres": "3.4.4",
		"reflect-metadata": "0.2.2",
		"rimraf": "6.0.1",
		"rxjs": "7.8.1",
		"sharp": "^0.33.4",
		"swagger-ui-express": "5.0.1",
		"typeorm": "0.3.20",
		"typevert": "0.9.11"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3.1.0",
